{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "NRM",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		},
		{
			"include": "#numbers"
		},
		{
			"include": "#operators"
		},
		{
			"include": "#functions"
		}
	],
	"repository": {
		"operators": {
			"name": "keyword.operator",
			"match": "(\\+|\\-|\\/|\\*|\\=|\\$|\\<|\\>|\\!)"
		},
		"comments": {
			"patterns": [
				{
					"name": "comment.block.hash",
					"begin": "!!",
					"end": "!!"
				}
			]
		},
		"keywords": {
			"patterns": [
				{
					"name": "keyword",
					"match": "\\b(let|constant|variable|is|be|now|null|true|false|create|changeable|unchangeable|function)\\b"
				}
			]
		},
		"strings": {
			"name": "string",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.ss",
					"match": "\\\\."
				}
			]
		},
		"numbers": {
			"patterns": [
				{
					"name": "constant.numeric.integer.ecx",
					"match": "\\b\\d+\\b"
				}
			]
		},
		"functions": {
			"name": "entity.name.function",
			"match": "\\b(\\w+)\\s*\\("
		}
	},
	"scopeName": "source.ec"
}
